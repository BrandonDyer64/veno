#!/usr/bin/python3
### statistics on programs
import cProfile
from pstats import Stats
prof = cProfile.Profile()
prof.enable()
### legacy from venic
import traceback
from modules.engine import Engine
scheduler = Engine()
while True:
	try:
		scheduler.turn()
	except:
		scheduler.terminate()
		str(traceback.print_exc())  # Print the exception
		break
